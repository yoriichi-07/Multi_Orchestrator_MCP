# Multi-Agent Orchestrator MCP Server Environment Configuration

# =============================================================================
# REQUIRED: Competition Technology Stack
# =============================================================================

# Descope Authentication (OAuth 2.1 + PKCE)
DESCOPE_PROJECT_ID=P2xxx...your-project-id
DESCOPE_CLIENT_SECRET=CS_xxx...your-client-secret  
DESCOPE_MANAGEMENT_KEY=K_xxx...your-management-key

# Cequence AI Gateway (Analytics & Monitoring)
CEQUENCE_GATEWAY_ID=gateway-xxx
CEQUENCE_API_KEY=ck_xxx...your-api-key

# =============================================================================
# OPTIONAL: AI Provider Configuration
# =============================================================================

# OpenAI API (for GPT models)
OPENAI_API_KEY=sk-xxx...your-openai-key

# Anthropic API (for Claude models)  
ANTHROPIC_API_KEY=sk-ant-xxx...your-anthropic-key

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================

# Debug mode (true/false)
DEBUG=false

# Maximum concurrent agents
MAX_AGENTS=5

# Enable self-healing capabilities (true/false)
HEALING_ENABLED=true

# Enable Cequence analytics collection (true/false)
ANALYTICS_ENABLED=true