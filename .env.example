# Multi-Agent Orchestrator MCP Server Configuration
# Copy this file to .env and fill in your actual values

# Required: Descope Authentication
DESCOPE_PROJECT_ID=your_descope_project_id_here
DESCOPE_CLIENT_SECRET=your_client_secret_here  
DESCOPE_MANAGEMENT_KEY=your_management_key_here

# Required: Cequence AI Gateway
CEQUENCE_GATEWAY_ID=your_cequence_gateway_id
CEQUENCE_API_KEY=your_cequence_api_key

# AI Provider Keys (at least one required)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Optional Configuration
DEBUG=false
MAX_AGENTS=5
HEALING_ENABLED=true
ANALYTICS_ENABLED=true

# Competition Environment
ENVIRONMENT=competition
MCP_VERSION=2.0.0